  "description": "dexwebapp new version",
  "private": true,
  "dependencies": {
    "@loopring-web/common-resources": "1.0.0",
    "@loopring-web/component-lib": "1.0.0",
    "@loopring-web/core": "1.0.0",
    "@manaflair/redux-batch": "^1.0.0",
    "clipboard": "^2.0.8",
    "ethers": "^5.5.4",
    "export-from-json": "^1.4.0",
    "lodash": "^4.17.20",
    "ms.macro": "^2.0.0",
    "react-device-detect": "^1.17.0",
    "react-router-dom": "^5.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "snarkjs": "0.1.20",
    "web-vitals": "^0.2.4"
  },
  "dependencies": {
    "@loopring-web/common-resources": "1.0.0",
    "@loopring-web/component-lib": "1.0.0",
    "@loopring-web/core": "1.0.0",
    "react-scripts": "4.0.3",
    "cross-env": "^7.0.3"
  },
  "resolutions": {
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "custom",
      "repo": "https://github.com/Loopring/loopring-web-v2",
      "owner": "Loopring Dex Frontend Team"
    }
  },
  "main": "./public/electron.js",
  "homepage": ".",
  "scripts": {
    "prettier": "prettier . --write",
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && git rev-parse --short HEAD; cross-env REACT_APP_VER=`git rev-parse --short HEAD`_prod dotenv -e .env.production  react-app-rewired build",
    "dev": "export SET NODE_OPTIONS=--openssl-legacy-provider && cross-env REACT_APP_VER=\"git rev-parse --short HEAD\"_dev dotenv -e .env.development react-app-rewired start",
    "build_dev": "REACT_APP_VER=`git rev-parse --short HEAD`_dev dotenv -e .env.development react-app-rewired build"
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
